void setup()
{
  pinMode(enB, OUTPUT);
  pinMode(in3, OUTPUT);
  pinMode(in4, OUTPUT);
  pinMode(8, INPUT_PULLUP); //Stopping motors

  Serial.begin(9600);
  Serial.println("Starting sketch.");

  //Qwicc motor driver settings
  myMotorDriver.settings.commInterface = I2C_MODE;

  //Setting address
  myMotorDriver.settings.I2CAddress = 0x5D;


  while ( myMotorDriver.begin() != 0xA9 )                                   //Code used from example libraries for easier debugging and problem solving, from here.
  {
    Serial.println( "ID mismatch, trying again" );
    delay(500);
  }
  Serial.println( "ID matches 0xA9" );

  Serial.print("Waiting for enumeration...");
  while ( myMotorDriver.ready() == false );
  Serial.println("Done.");
  Serial.println();

  //Motor inversion                                                             //Except this, this is used for inverting motor direction.
  while ( myMotorDriver.busy() );
  myMotorDriver.inversionMode(1, 0); //invert motor 1
  while ( myMotorDriver.busy() );
  myMotorDriver.enable(); //Enables the output driver hardware

  if (!display.begin(SSD1306_SWITCHCAPVCC, 0x3D)) { // Address 0x3D for 128x64
    Serial.println(F("SSD1306 allocation failed"));
    for (;;); // Don't proceed, loop forever                                           //To here
  }

  //Clear the display
  display.clearDisplay();


}

#define LEFT_MOTOR 0
#define RIGHT_MOTOR 1

int xx = 5;
int yy = 0;
int tt = 0;

void loop()
{

  //directionControl();
  myMotorDriver.setDrive( LEFT_MOTOR, 0, 0); //Stop motor
  myMotorDriver.setDrive( RIGHT_MOTOR, 0, 0); //Stop motor
  while (digitalRead(8) == 0); //Hold if jumper is placed between pin 8 and ground

  delay(5000);                                       //Delay 5 seconds to give me the chance to check if recording is ready
  forward();                                         //Moving the robot forward for 3 seconds
  delay(3000);
  halt();                                            //Stopping it
  delay(1000);
  right();                                           //After one second, turn it to the right.
  delay(700);
  halt();                                            //Stop the movement, and start moving head left and right 50 times, animate the OLED.
  for (int i = 0; i < 50; i++) {
    headMove();
    smile();
  }

}                                                    //This was the code for scene one, after that everything was done by simply commenting and uncommenting parts of the code whih I needed for the next scenes, or needed them removed.
                                                     //For example, when I needed only the head to turn, all of the other movements were commented.
  

