//Moving the head from left to right
void headMove() {
  analogWrite(enB, 40);
  digitalWrite(in3, HIGH);
  digitalWrite(in4, LOW);
  delay(300);
  //Change direction
  digitalWrite(in3, LOW);
  digitalWrite(in4, HIGH);
  delay(300);
}

//Move forward
void forward() {
  myMotorDriver.setDrive( LEFT_MOTOR, 1, 200);
  myMotorDriver.setDrive( RIGHT_MOTOR, 1, 130);
  delay(5);
}

//Move left
void left() {
  myMotorDriver.setDrive( LEFT_MOTOR, 1, 200);
  myMotorDriver.setDrive( RIGHT_MOTOR, 1, 0);
  delay(5);
}
//Move right
void right() {
  myMotorDriver.setDrive( LEFT_MOTOR, 1, 0);
  myMotorDriver.setDrive( RIGHT_MOTOR, 1, 150);
  delay(5);
}
//Stop
void halt() {
  myMotorDriver.setDrive( LEFT_MOTOR, 1, 0);
  myMotorDriver.setDrive( RIGHT_MOTOR, 1, 0);
  delay(5);
}
